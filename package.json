        "antd": "^5.22.5",
        "aws-sdk-client-mock": "^4.1.0",
        "dayjs": "^1.11.13",
        "jwt-decode": "^4.0.0",
        "less": "^4.2.1",
        "next": "15.0.4",
        "node-emoji": "^2.2.0",
        "oidc-client-ts": "^3.2.0",
        "react": "^19.0.0",
