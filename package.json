{
    "name": "wordless-client",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build",
        "start": "next start",
        "lint": "run-p --continue-on-error --print-label lint:*",
        "lint:eslint": "next lint",
        "lint:prettier": "prettier --cache --check .",
        "lint:secretlint": "secretlint --maskSecrets --secretlintignore .gitignore '**/*'",
        "fix": "run-s --continue-on-error fix:*",
        "fix:eslint": "next lint --fix",
        "fix:prettier": "prettier --cache --write .",
        "prepare": "panda codegen"
    },
    "dependencies": {
        "@ant-design/icons": "^5.5.2",
        "antd": "^5.22.5",
        "less": "^4.2.1",
        "next": "15.0.4",
        "node-emoji": "^2.2.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@pandacss/dev": "^0.49.0",
        "@secretlint/secretlint-rule-preset-recommend": "^9.0.0",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "eslint": "^8",
        "eslint-config-next": "15.0.4",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "npm-run-all2": "^7.0.1",
        "pnpm": "^9.15.0",
        "prettier": "^3.4.2",
        "secretlint": "^9.0.0",
        "typescript": "^5",
        "typescript-eslint": "^8.17.0"
    }
}
