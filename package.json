        "jsdom": "^26.0.0",
        "msw": "^2.7.3",
        "npm-run-all2": "^7.0.1",
        "pnpm": "^9.15.0",
        "prettier": "^3.4.2",
        "secretlint": "^9.0.0",
        "typescript": "^5",
        "typescript-eslint": "^8.17.0",
